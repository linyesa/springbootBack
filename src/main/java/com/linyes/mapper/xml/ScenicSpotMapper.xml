        <id column="id" property="id" />
        <result column="scenic_name" property="scenicName" />
        <result column="scenic_address" property="scenicAddress" />
        <result column="scenic_describe" property="scenicDescribe" />
        <result column="scenic_image" property="scenicImage" />
        <result column="longitude" property="longitude" />
        <result column="latitude" property="latitude" />
        <result column="prov" property="prov" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, scenic_name, scenic_address, scenic_describe, scenic_image, longitude, latitude, prov
    </sql>

</mapper>
